#ifndef __LZL_UTILS__LZL_FUNCTION_TRAITS_HEADER__
#define __LZL_UTILS__LZL_FUNCTION_TRAITS_HEADER__

#include "functional/cxx17/function_traits.h"

namespace lzl {
template <typename Callable>
using function_traits = utils::function_traits<Callable>;
template <typename Func>
using trains_class_type = utils::trains_class_type<Func>;
} // namespace lzl

#endif // __LZL_UTILS__LZL_FUNCTION_TRAITS_HEADER__
